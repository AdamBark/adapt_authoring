<div class="location-title-inner">
	{{#if breadcrumbs}}
		<div class="breadcrumbs">
			{{#each breadcrumbs}}
				<div class="item">
					{{#if @last}}
						{{title}}
					{{else}}
						{{#if url}}
							<a href="{{url}}">{{title}}</a>
						{{else}}
							{{title}}
						{{/if}}
					{{/if}}
				</div>
				{{#if @last}}
				{{else}}
					<div class="separator unselectable">&gt;</div>
				{{/if}}
			{{/each}}
		</div>
	{{/if}}
	{{#if title}}
		{{#if course}}
			<div class="course">
				{{console course}}
				{{course.title}}
			</div>
			<div class="separator unselectable">&gt;</div>
		{{/if}}
		<div class="title">
			{{title}}
		</div>
	{{/if}}
</div>
